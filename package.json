{
  "name": "unstorage-s3-driver",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.6.2",
  "description": "A custom Unstorage driver for AWS S3.",
  "author": "Kyle Lauber <linkdevk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kylegl/unstorage-s3-driver#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylegl/unstorage-s3-driver.git"
  },
  "bugs": "https://github.com/kylegl/unstorage-s3-driver/issues",
  "keywords": [
    "unstorage",
    "driver",
    "storage",
    "s3",
    "aws",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "unstorage": "^1.10.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "catalog:driver"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/ni": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "eslint": "catalog:cli",
    "lint-staged": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:utils",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli",
    "unstorage": "catalog:driver",
    "vite": "catalog:cli",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
